{
    "name":"webCrawer helper",
    "description":"点击获取cherrio格式的node",
    "version":"0.0.1",
    "icons": { 
        "16": "image/seckill-16.png",
        "48": "image/seckill-48.png",
        "128": "image/seckill-128.png" 
        },
    "permissions": [
        "tabs",
        "storage",
        "webNavigation",
        "notifications",
        "activeTab",
        "cookies",
        "http://*/*",
        "https://*/*"
        ],
    "background": {
        "scripts": ["lib/vue.js","js/background.js"]
    },
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"], 
          "js": [ "js/locate.js"]
        }
    ],
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "image/seckill-16.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://sapi.k780.com/; object-src 'self'",
    "web_accessible_resources": ["lib/*.js","js/*.js"]
}