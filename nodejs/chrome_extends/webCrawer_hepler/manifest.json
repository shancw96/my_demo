{
    "name": "TB_order_killer",
    "description": "加入购物车，选中，自动抢单~",
    "version": "0.0.1",
    "icons": {
        "16": "image/seckill-16.png",
        "48": "image/seckill-48.png",
        "128": "image/seckill-128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "webNavigation",
        "notifications",
        "activeTab",
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/contentScript/locate.js",
                "js/contentScript/autoTriggle.js"
            ]
        }
    ],
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "image/seckill-16.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://limiaomiao.site/; object-src 'self'",
    "web_accessible_resources": [
        "lib/*.js",
        "js/*.js"
    ]
}